{
  "expo": {
    "name": "Stikom Mobile",
    "slug": "stikom-mobile",
    "scheme": "com.stikom.mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A91FF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stikom.mobile",
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A91FF"
      },
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": [{ "scheme": "msauth" }],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.stikom.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "config": {
        "firebase": {
          "projectId": "stikom-mobile",
          "apiKey": "AIzaSyB2436gv_7Nm8lPlWljeSk-GlFx62VDXwU",
          "measurementId": "G-Z8CDK9XG3M"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./themes/fonts/Inter-Medium.ttf",
            "./themes/fonts/Inter-SemiBold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d5d2e70-2698-446c-a7b7-d94b738412d4"
      }
    },
    "owner": "krisnapy533"
  }
}
